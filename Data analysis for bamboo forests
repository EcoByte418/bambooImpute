create_boxplot <- function(df, factor_col, numeric_col, label, letter) {
  ggplot(df, aes(x = .data[[factor_col]], y = .data[[numeric_col]])) +
    geom_boxplot() +
    theme_bw() +
    labs(title = letter, x = NULL, y = label) +
    theme(legend.position = "none")
}


